{
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "python.formatting.provider": "none",
  "spellright.language": [],
  "spellright.documentTypes": [
    "markdown",
    "latex",
    "plaintext",
    "Log"
  ],
  "spellright.parserByClass": {
    "Log": {
      "parser": "plain"
    }
  },
  "C_Cpp.errorSquiggles": "enabled",
  "files.associations": {
    "__locale": "c"
  }
}